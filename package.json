    "superagent": "^6.1.0",
    "tweetnacl": "^1.0.3",
    "url-parse": "^1.5.1"
  },
  "devDependencies": {
    "@types/json-bigint": "^1.0.0",
    "@types/mocha": "^8.2.1",
    "@types/superagent": "^4.1.10",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "assert": "^2.0.0",
