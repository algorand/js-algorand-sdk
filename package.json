{
  "name": "algosdk",
  "version": "1.8.1",
  "description": "algosdk is Algorand's official javascript SDK",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/browser/algosdk.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "directories": {
